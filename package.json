{
	"name": "kysely-kinetic",
	"version": "1.0.0",
	"description": "",
	"author": "Marcos Essindi",
	"license": "MIT",
	"homepage": "https://github.com/dunkelbraun/kysely-kinetic",
	"type": "module",
	"main": "dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"bin": {
		"kinetic": "dist/esm/src/bin/cli.js",
		"kinetic-autopilot": "dist/esm/src/bin/autopilot.js"
	},
	"exports": {
		".": {
			"import": "./dist/esm/src/index.js",
			"require": "./dist/cjs/src/index.js",
			"types": "./dist/esm/src/index.d.ts"
		},
		"./autopilot": {
			"import": "./dist/esm/src/autopilot.js",
			"require": "./dist/cjs/src/autopilot.js",
			"types": "./dist/esm/src/autopilot.d.ts"
		},
		"./config": {
			"import": "./dist/esm/src/config.js",
			"require": "./dist/cjs/src/config.js",
			"types": "./dist/esm/src/config.js"
		},
		"./pool": {
			"import": "./dist/esm/src/database/pg/pg_pool.js",
			"require": "./dist/cjs/src/database/pg/pg_pool.js",
			"types": "./dist/esm/src/database/pg/pg_pool.js"
		},
		"./zod": {
			"import": "./dist/esm/src/database/schema/zod.js",
			"require": "./dist/cjs/src/database/schema/zod.js",
			"types": "./dist/esm/src/database/schema/zod.js"
		}
	},
	"types": "./dist/esm/src/index.d.ts",
	"files": [
		"dist/**/*"
	],
	"scripts": {
		"prebuild": "rm -rf dist",
		"build": "npm run build:esm && npm run build:cjs",
		"build:cjs": "tsc -p tsconfig.cjs.json --outDir dist/cjs",
		"build:esm": "tsc -p tsconfig.esm.json --outDir dist/esm",
		"postbuild": "npm run postbuild:makeexec && npm run postbuild:packagecjs && npm run postbuild:packageesm && npm run postbuild:transform-paths",
		"postbuild:transform-paths": "tsc-alias -p tsconfig.json --outDir dist/esm && tsc-alias -p tsconfig.cjs.json --outDir dist/cjs",
		"postbuild:makeexec": "chmod +x dist/esm/src/bin/cli.js dist/esm/src/bin/autopilot.js",
		"postbuild:packagecjs": "echo '{\n  \"type\": \"commonjs\"\n}' > dist/cjs/src/package.json",
		"postbuild:packageesm": "echo '{\n  \"type\": \"module\"\n}' > dist/esm/src/package.json",
		"check": "npm run check:type && npm run check:prettier && npm run check:lint",
		"check:lint": "npx eslint .",
		"check:prettier": "prettier --check .",
		"check:type": "./tests/bin/type-check",
		"cli": "src/cli/cli.ts",
		"format": "prettier --write .",
		"prepack": "npm run check && npm run build && rm -rf build && mkdir build",
		"pack": "npm pack --pack-destination ./build",
		"test": "vitest unit src integration --run --coverage",
		"pretest:unit": "tsx ./tests/setup/create_test_db.ts",
		"test:unit": "./tests/bin/run-tests",
		"posttest:unit": "tsx ./tests/setup/drop_test_db.ts",
		"pretest:integration": "npm run pack && tsx ./tests/setup/create_test_db.ts",
		"posttest:integration": "tsx ./tests/setup/drop_test_db.ts",
		"test:integration": "vitest integration --run --coverage",
		"pretest:watch": "tsx ./tests/setup/create_test_db.ts",
		"test:watch": "vitest unit src --watch --ui --open --coverage"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/dunkelbraun/kysely-kinetic.git"
	},
	"bugs": {
		"url": "https://github.com/dunkelbraun/kysely-kinetic/issues"
	},
	"devDependencies": {
		"@types/mock-fs": "^4.13.4",
		"@types/node": "^18.0.0",
		"@types/nunjucks": "^3.2.6",
		"@types/pg": "^8.11.0",
		"@types/slug": "^5.0.7",
		"@types/toposort": "^2.0.7",
		"@typescript-eslint/eslint-plugin": "^7.1.1",
		"@typescript-eslint/parser": "^7.1.1",
		"@vitest/coverage-istanbul": "^1.3.1",
		"@vitest/ui": "^1.3.1",
		"dotenv": "^16.4.1",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-react": "^7.34.0",
		"kysely-0270": "npm:kysely@^0.27.0",
		"kysely-0271": "npm:kysely@^0.27.1",
		"kysely-0272": "npm:kysely@^0.27.2",
		"kysely-0273": "npm:kysely@^0.27.3",
		"mock-fs": "^5.2.0",
		"mock-stdin": "^1.0.0",
		"postcss": "^8.4.35",
		"prettier": "3.2.5",
		"prettier-plugin-organize-imports": "^3.2.4",
		"tsc-alias": "^1.8.8",
		"type-fest": "^4.10.1",
		"type-testing": "^0.2.0",
		"typescript-504": "npm:typescript@5.0.4",
		"typescript-516": "npm:typescript@5.1.6",
		"typescript-522": "npm:typescript@5.2.2",
		"typescript-533": "npm:typescript@5.3.3",
		"typescript-542": "npm:typescript@5.4.2",
		"vite-tsconfig-paths": "^4.3.1",
		"vitest": "^1.3.1"
	},
	"dependencies": {
		"@clack/prompts": "^0.7.0",
		"@commander-js/extra-typings": "^12.0.1",
		"commander": "12.0.0",
		"execa": "^8.0.1",
		"microdiff": "^1.3.2",
		"nunjucks": "^3.2.4",
		"picocolors": "^1.0.0",
		"slug": "^8.2.3",
		"toposort": "^2.0.2",
		"tsx": "^4.7.0",
		"zod": "^3.22.2"
	},
	"peerDependencies": {
		"kysely": "^0.27.3",
		"pg": ">=8.11.3"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
