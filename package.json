{
  "name": "kysely-kinetic",
  "version": "1.0.0",
  "description": "",
  "author": "Marcos Essindi",
  "license": "MIT",
  "homepage": "https://github.com/dunkelbraun/kysely-kinetic",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "bin": {
    "kinetic": "dist/esm/src/cli/index.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prebuild": "rm -rf dist && npm run check",
    "build": "npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module es2022 --outDir dist/esm",
    "postbuild": "chmod +x ./dist/esm/src/cli/index.js",
    "check": "npm run check:type && npm run check:biome",
    "check:biome": "biome check .",
    "check:type": "tsc --noEmit",
    "cli": "src/cli/index.ts",
    "format": "biome format --write .",
    "prepack": "npm run build && rm -rf build && mkdir build",
    "pack": "npm pack --pack-destination ./build",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest unit --run --coverage",
    "pretest:integration": "npm run pack",
    "test:integration": "vitest integration --run --coverage",
    "test:watch": "vitest unit --watch --ui --open --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dunkelbraun/kysely-kinetic.git"
  },
  "bugs": {
    "url": "https://github.com/dunkelbraun/kysely-kinetic/issues"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/node": "^18.0.0",
    "@vitest/coverage-istanbul": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "ts-node": "^10.9.2"
  }
}
