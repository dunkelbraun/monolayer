{
  "name": "kysely-kinetic",
  "version": "1.0.0",
  "description": "",
  "author": "Marcos Essindi",
  "license": "MIT",
  "homepage": "https://github.com/dunkelbraun/kysely-kinetic",
  "main": "dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "prebuild": "rm -rf dist && npm run check",
    "build": "npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module es2022 --outDir dist/esm",
    "check": "npm run check:type && npm run check:biome",
    "check:biome": "biome check .",
    "check:type": "tsc --noEmit",
    "format": "biome format --write .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dunkelbraun/kysely-kinetic.git"
  },
  "bugs": {
    "url": "https://github.com/dunkelbraun/kysely-kinetic/issues"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/node": "^18.0.0",
    "typescript": "^5.3.3"
  }
}
