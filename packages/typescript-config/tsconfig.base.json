{
	"compilerOptions": {
		"target": "es2022",
		"lib": ["es2023"],
		"module": "Node16",
		"moduleResolution": "Node16",
		"baseUrl": "./",
		"declaration": true,
		"sourceMap": true,
		"stripInternal": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noUncheckedIndexedAccess": true,
		"noUnusedLocals": true,
		"skipLibCheck": true
	}
}
