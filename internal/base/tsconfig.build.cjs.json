{
	"extends": "./tsconfig.json",
	"include": ["src/**/*"],
	"compilerOptions": {
		"emitDeclarationOnly": false,
		"sourceMap": false,
		"rootDir": "src",
		"outDir": "dist/cjs",
		"target": "es2020",
		"module": "CommonJS",
		"moduleResolution": "Node",
		"incremental": true,
		"tsBuildInfoFile": "dist/tsconfig.build.cjs.tsbuildinfo"
	}
}
